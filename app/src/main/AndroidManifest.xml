        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.NoActionBar" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".glActivity.TextureTriangleActivity"/>
        <activity android:name=".glActivity.IDCardActivity"/>
        <activity android:name=".glActivity.CreateIDCardActivity"/>
        <activity android:name=".glActivity.SkyBoxActivity"/>
    </application>


</manifest>
